<?xml version="1.0" encoding="UTF-8"?>
<module>
  <inherits name="com.google.gwt.user.User" />
  <inherits name="org.jbox2d.GwtBox2D"/>
  <inherits name='com.google.gwt.http.HTTP'/>
  <inherits name='com.google.gwt.json.JSON'/>
  <inherits name="com.googlecode.gwt.crypto.Crypto"/>
  
  <source path="asset"/>
  <source path="service"/>
  <source path="component"/>
  <source path="core"/>
  <source path="game"/>
  <source path="developmenttools"/>
  <source path="page"/>
  <source path="physics"/>
  <source path="sprite"/>
  <source path="timer"/>
  <source path="layer"/>
  
  <generate-with class="com.google.gwt.maeglin89273.game.mengine.rebind.AssetsBundleGenerator">
  	<when-type-assignable class="com.google.gwt.maeglin89273.game.mengine.asset.AssetsBundleWithLookup"/>
  </generate-with>
  <define-linker name="appcache" class="com.google.gwt.maeglin89273.game.mengine.rebind.AppCacheLinker"/>
  <set-property name="ClientBundle.enableInlining" value="false" />
  
  <!--<add-linker name="appcache"/>-->
  
</module>
